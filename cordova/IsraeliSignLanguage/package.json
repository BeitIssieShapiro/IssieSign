{
    "name": "IsraeliSignLanguage",
    "displayName": "The Israeli Sign Language",
    "version": "1.0.0",
    "description": "An app that help learning the sign language of Israel",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "SAP CSR Team",
    "license": "Apache-2.0",
    "dependencies": {
        "com.flyingsoftgames.xapkreader": "https://github.com/agamemnus/cordova-plugin-xapkreader.git#cordova-6.5.0",
        "cordova-android": "^8.0.0",
        "cordova-ios": "^5.0.1",
        "cordova-plugin-camera": "^4.1.0",
        "cordova-plugin-device": "^2.0.3",
        "cordova-plugin-file": "^6.0.1",
        "cordova-plugin-image-picker": "^1.1.3",
        "cordova-plugin-splashscreen": "^5.0.2",
        "cordova-plugin-whitelist": "^1.3.3",
        "cordova-plugin-xapkreader": "git+https://github.com/agamemnus/cordova-plugin-xapkreader.git#cordova-6.5.0"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-file": {},
            "com.flyingsoftgames.xapkreader": {
                "XAPK_PUBLIC_KEY": "",
                "XAPK_EXPANSION_AUTHORITY": "com.issieshapiro.issiesign",
                "XAPK_AUTO_DOWNLOAD": "true",
                "XAPK_AUTO_PERMISSION": "true",
                "XAPK_TEXT_DL_ASSETS": "מוריד קבצי מדיה",
                "XAPK_MAIN_VERSION": "0"
            },
            "cordova-plugin-device": {},
            "cordova-plugin-image-picker": {
                "PHOTO_LIBRARY_USAGE_DESCRIPTION": " "
            },
            "cordova-plugin-camera": {
                "ANDROID_SUPPORT_V4_VERSION": "27.+"
            }
        },
        "platforms": [
            "android",
            "ios"
        ]
    }
}